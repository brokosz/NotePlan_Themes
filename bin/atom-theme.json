{
  "name": "Atom theme",
  "style": "Dark",
  "author": {
    "name": "S",
    "email": "sulejdev@gmail.com"
  },

  "editor": {
    "backgroundColor": "#282C34",
    "altBackgroundColor": "#262A33",
    "tintColor": "#61AFFF",
    "tintColor2": "#ECCB84",
    "textColor": "#C7C6CC",
    "toolbarBackgroundColor": "#434551",
    "toolbarIconColor": "#A0A0A8",
    "menuItemColor": "#A0A0A8",
    "shouldOverwriteFont": true
  },

  "styles": {

    "body": {
      "font": "AvenirNext-Regular",
      "size": 16,
      "color": "#C7C6CC",
    },

    "title1": {
      "color": "#EB7381",
      "size": 28,
      "font": "AvenirNext-DemiBold"
    },

    "title2": {
      "color": "#EB7381",
      "size": 24,
      "font": "AvenirNext-DemiBold"
    },

    "title3": {
      "color": "#EB7381",
      "size": 20,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark1": {
      "color": "#45EB7381",
      "size": 28,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark2": {
      "color": "#45EB7381",
      "size": 24,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark3": {
      "color": "#45EB7381",
      "size": 20,
      "font": "AvenirNext-DemiBold"
    },

    "bold": {
      "font": "AvenirNext-Bold"
    },

    "bold-left-mark": {
      "color": "#45C7C6CC"
    },

    "bold-right-mark": {
      "color": "#45C7C6CC"
    },

    "italic": {
      "font": "AvenirNext-Italic"
    },

    "italic-left-mark": {
      "color": "#45C7C6CC"
    },

    "italic-right-mark": {
      "color": "#45C7C6CC"
    },

    "boldItalic": {
      "font": "AvenirNext-BoldItalic"
    },

    "boldItalic-left-mark": {
      "color": "#45C7C6CC"
    },

    "boldItalic-right-mark": {
      "color": "#45C7C6CC"
    },

    "code": {
      "font": "Menlo-Regular",
      "color": "#ECCB84",
      "size": 15
    },

    "code-left-backtick": {
      "color": "#66ECCB84"
    },

    "code-right-backtick": {
      "color": "#66ECCB84"
    },

    "checked": {
      "color": "#66C7C6CC",
      "headIndent": 33
    },

    "todo": {
      "font": "noteplanstate",
      "color": "#9EC87C",
      "headIndent": 33,
      "size": 16
    },

    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },

    "special-char": {
      "font": "Menlo-Regular"
    },

    "tabbed": {
      "headIndent": 43
    },

    "quote-mark": {
      "color": "#80ECCB84",
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },

    "quote-content": {
      "color": "#C678DD",
      "type": "italic",
      "headIndent": 33
    },

    "link": {

    },

    "schedule-to-date-link": {

    },

    "done-date": {

    },

    "schedule-from-date-link": {

    },

    "note-title-link": {

    },

    "hashtag": {

    },

    "attag": {

    },

    "phonenumber": {

    }
  }
}
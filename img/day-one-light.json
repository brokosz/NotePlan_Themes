{
    "name": "Day One",
    "style": "Light",
    "author": {
        "name": "Kennon Bickhart",
        "email": "kennon@bickh.art"
    },

    "editor": {
        "backgroundColor": "#FFFFFF",
        "altBackgroundColor": "#FFFFFF",
        "tintColor": "#44c0ff",
        "tintColor2": "#44c0ff",
        "textColor": "#333b40",
        "toolbarBackgroundColor": "#FFFFFF",
        "toolbarIconColor": "#44c0ff",
        "menuItemColor": "#44c0ff",
        "shouldOverwriteFont": false
    },

    "styles": {

        "body": {
            "size": 16,
            "lineSpacing": 10,
            "paragraphSpacing": 0,
            "paragraphSpacingBefore": 0,
            "color": "#333b40"
        },

        "title1": {
            "color": "#22282b",
            "size": 26,
            "font": ".AppleSystemUIFontBold"
        },

        "title2": {
            "color": "#22282b",
            "size": 23,
            "font": ".AppleSystemUIFontBold"
        },

        "title3": {
            "regex": "^\\h*(### )(.*)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "color": "#e74c3d",
            "size": 21,
            "font": ".AppleSystemUIFontBold"
        },

        "title4": {
            "regex": "^\\h*(#### )(.*)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "color": "#c2c3c4",
            "size": 16
        },

        "title5": {
            "regex": "^\\h*(##### )(.*)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "color": "#333b40",
            "size": 16,
            "underlineStyle": 3,
            "underlineColor": "#e9e9e9"
        },

        "title6": {
            "regex": "^\\h*(######+ )(.*)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#f5f5f5",
            "color": "#333b40",
            "size": 14
        },

        "title-mark1": {
            "color": "#5022282B",
            "size": 16,
            "paragraphSpacing": 8,
            "paragraphSpacingBefore": 36
        },

        "title-mark2": {
            "color": "#5022282B",
            "size": 16,
            "paragraphSpacing": 8,
            "paragraphSpacingBefore": 36
        },

        "title-mark3": {
            "regex": "^\\h*(### )",
            "matchPosition": 1,
            "isRevealOnCursorRange": true,
            "color": "#50e74c3d",
            "size": 16,
            "paragraphSpacing": 8,
            "paragraphSpacingBefore": 36
        },

        "title-mark4": {
            "regex": "^\\h*(#### )",
            "matchPosition": 1,
            "isRevealOnCursorRange": true,
            "isHiddenWithoutCursor": true,
            "isMarkdownCharacter": true,
            "color": "#70c2c3c4",
            "size": 14,
            "paragraphSpacing": 8,
            "paragraphSpacingBefore": 36
        },

        "title-mark5": {
            "regex": "^\\h*(##### )",
            "matchPosition": 1,
            "isRevealOnCursorRange": true,
            "isHiddenWithoutCursor": true,
            "isMarkdownCharacter": true,
            "color": "#5022282B",
            "size": 13,
            "paragraphSpacing": 4,
            "paragraphSpacingBefore": 36
        },

        "title-mark6": {
            "regex": "^\\h*(######+ )",
            "matchPosition": 1,
            "isRevealOnCursorRange": true,
            "isHiddenWithoutCursor": true,
            "isMarkdownCharacter": true,
            "color": "#5022282B",
            "size": 13,
            "paragraphSpacing": 4,
            "paragraphSpacingBefore": 36
        },

        "bold": {
            "color": "#333b40",
            "font": ".AppleSystemUIFontBold"
        },

        "bold-left-mark": {
            "color": "#50333b40"
        },

        "bold-right-mark": {
            "color": "#50333b40"
        },

        "italic": {
            "color": "#333b40",
            "font": "AvenirNext-Italic"
        },

        "italic-left-mark": {
            "color": "#50333b40"
        },

        "italic-right-mark": {
            "color": "#50333b40"
        },

        "boldItalic": {
            "font": ".AppleSystemUIFontBoldItalic"
        },

        "boldItalic-left-mark": {
            "color": "#50333b40"
        },

        "boldItalic-right-mark": {
            "color": "#50333b40"
        },

        "code": {
            "font": "JetBrainsMono-Regular",
            "size": 15,
            "backgroundColor": "#f5f5f5"
        },

        "code-left-backtick": {
            "color": "#ccc"
        },

        "code-right-backtick": {
            "color": "#ccc"
        },

        "checked": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
            "matchPosition": 0,
            "color": "#9ac476",
            "headIndent": 33
        },

        "checked-canceled": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
            "matchPosition": 0,
            "color": "#cacacf",
            "headIndent": 33,
            "strikethroughStyle": 1
        },

        "checked-scheduled": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
            "matchPosition": 0,
            "color": "#c27bd2",
            "headIndent": 33
        },

        "todo": {
            "font": "noteplanstate",
            "color": "#c2c3c4",
            "headIndent": 33,
            "size": 16
        },

        "checked-todo-characters": {
            "font": "noteplanstate",
            "headIndent": 33,
            "size": 16
        },

        "list-number": {
            "regex": "(^|\\v|\\t)([0-9]+\\.)",
            "matchPosition": 2,
            "color": "#cacacf",
            "font": ".AppleSystemUIFont"
        },

        "list-bullet": {
            "regex": "^((\\-|\\*)+)(?!\\s\\[(\\s|x|\\>|\\-)\\])",
            "matchPosition": 1,
            "color": "#44c0ff"
        },

        "special-char": {
            "font": "JetBrainsMono-Regular"
        },

        "tabbed": {
            "headIndent": 43
        },

        "quote-mark": {
            "color": "#e7eaec",
            "font": "noteplanstate",
            "headIndent": 33,
            "size": 16
        },

        "quote-content": {
            "color": "#95a5a6",
            "type": "italic",
            "font": ".AppleSystemUIFontItalic",
            "headIndent": 33
        },

        "quote-citation": {
            "regex": "(~{1,})(\\s.+)",
            "matchPosition": 2,
            "font": ".AppleSystemUIFontBold"
        },

        "link": {},
        "schedule-to-date-link": {},
        "done-date": {},
        "schedule-from-date-link": {},
        "note-title-link": {},
        "hashtag": {},
        "attag": {},

        "highlighted": {
            "regex": "(::)([^:]{1,})(::)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#ffe08e",
            "textColor": "#333b40"
        },

        "highlighted-left-colon": {
            "regex": "(::)([^:]{1,})(::)",
            "matchPosition": 1,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true,
            "color": "#333b40",
            "backgroundColor": "#70ffe08e"
        },

        "highlighted-right-colon": {
            "regex": "(::)([^:]{1,})(::)",
            "matchPosition": 3,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true,
            "color": "#333b40",
            "backgroundColor": "#70ffe08e"
        },

        "strikethrough": {
            "regex": "(~~)([^~]{1,})(~~)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "strikethroughStyle": 1
        },

        "strikethrough-left-mark": {
            "regex": "(~~)([^~]{1,})(~~)",
            "matchPosition": 1,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true
        },

        "strikethrough-right-mark": {
            "regex": "(~~)([^~]{1,})(~~)",
            "matchPosition": 3,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true
        },

        "flagged-1": {
            "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{1}\\h+.*)($|\\v)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#22e74c3d",
            "headIndent": 33
        },

        "flagged-2": {
            "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{2}\\h+.*)($|\\v)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#55e74c3d",
            "color": "#333b40",
            "headIndent": 33
        },

        "flagged-3": {
            "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{3}\\h+.*)($|\\v)",
            "matchPosition": 2,
            "isRevealOnCursorRange": true,
            "backgroundColor": "#aae74c3d",
            "headIndent": 33
        },

        "flagged-check-character": {
            "regex": "^((\\-|\\*)\\s\\[\\s\\]\\s)(?=!+)",
            "matchPosition": 1,
            "backgroundColor": "#ffffff",
            "color": "#e74c3d",
            "headIndent": 33
        },

        "flagged-character": {
            "regex": "^((\\-|\\*)+)(\\s\\[\\s\\])(\\s\\!+\\s)",
            "matchPosition": 4,
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true,
            "headIndent": 33
        }
    }
}
